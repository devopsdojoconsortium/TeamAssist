    "@cycle/history": "latest",
    "@cycle/isolate": "^3.3.0",
    "@cycle/rxjs-run": "^4.0.0-rc.5",
    "@cycle/storage": "^5.0.3",
    "history": "latest",
    "jquery": "^3.3.1",
    "js-cookie": "2.2.0",
    "marked": "^0.4.0",
    "moment": "^2.22.2",
    "murmur": "0.0.2",
    "rxjs": "^5.5.11",
    "uuid": "^3.2.1",
    "xstream": "^11.7.0"
  },
  "devDependencies": {
    "apickli": "^2.1.0",
    "babel-plugin-transform-object-rest-spread": "^6.6.5",
    "babel-preset-es2015": "^6.18.0",
    "babel-register": "^6.4.3",
    "babelify": "^7.2.0",
    "browserify": "12.0.1",
    "chai": "^3.5.0",
    "chai-virtual-dom": "^1.0.0",
    "cucumber": "^4.1.0",
    "eslint": "^4.18.2",
    "eslint-watch": "^1.2.5",
    "jsonfile": "^4.0.0",
    "live-server": "^0.9.0",
    "mkdirp": "^0.5.1",
    "mocha": "5.2.0",
    "npm-run-all": "^1.4.0",
    "read-file": "0.2.0",
    "uglify-js": "3.1.5",
    "watchify": "^3.11.0"
  },
  "scripts": {
    "build-debug": "mkdirp dist && browserify src/app.js -d -t babelify --outfile dist/app.js",
    "cucumber": "cucumber-js src/test/cucumber/features/ -r src/test/cucumber/steps/",
    "watch:js": "mkdirp dist && watchify src/app.js -t babelify --outfile dist/app.js -dv",
    "serve": "live-server ./ --host=localhost --watch=dist/app.js --cors",
    "uglify": "uglifyjs dist/app.js -c drop_console=true -o dist/app.min.js",
    "build": "npm run build-debug && npm run uglify",
    "start": "npm-run-all --parallel watch:js  serve",
    "start-lim": "npm-run-all --parallel watch:js",
    "mocha": "mocha --compilers js:babel-core/register src/test/*.js || true",
    "mochaBab": "mocha src/test/*.js || true",
    "test": "npm run mocha && npm run lint",
    "lint": "eslint src/ || true",
    "wmocha": "mocha -w --compilers js:babel-core/register src/test/*.js || true",
    "wlint": "esw src/ --quiet -w",
    "jenkins-mocha": "mocha --compilers js:babel-core/register src/test/*.js"
  }
}
